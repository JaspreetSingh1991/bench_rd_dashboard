        "visual_type": "Card",
        "title": "Performance Metrics",
        "items": [
          {
            "label": "Success Rate",
            "value": "Success Rate",
            "color": "#10b981",
            "suffix": "%"
          },
          {
            "label": "Opportunity Density",
            "value": "Opportunity Density", 
            "color": "#3b82f6",
            "decimal_places": 2
          },
          {
            "label": "Data Quality",
            "value": "Data Quality",
            "color": "#8b5cf6"
          }
