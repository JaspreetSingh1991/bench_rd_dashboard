          "padding": "16px",
          "box_shadow": "0 1px 4px rgba(0,0,0,0.04)"
        }
      }
    }
  },
  "interactive_features": {
    "drillthrough": {
      "enabled": true,
      "pages": [
        {
          "name": "Screen Reject Details",
          "source_visual": "Screen Rejects Card",
          "target_page": "Screen Reject Details",
          "filters": ["Date", "Reason", "Candidate"]
        },
        {
          "name": "Tech Reject Details", 
          "source_visual": "Technical Rejects Card",
          "target_page": "Tech Reject Details",
          "filters": ["Skill", "Rating", "Candidate"]
        },
        {
          "name": "Opportunity Details",
          "source_visual": "Opportunities Table",
          "target_page": "Opportunity Details", 
          "filters": ["Role", "Candidate", "Status"]
        }
      ]
    },
    "filters": {
      "date_range": {
        "enabled": true,
        "visual_type": "Slicer",
        "field": "Date",
        "type": "Between"
      },
      "role_filter": {
        "enabled": true,
        "visual_type": "Slicer", 
        "field": "Role for which interviewed",
        "type": "List"
      },
      "candidate_status": {
        "enabled": true,
        "visual_type": "Slicer",
        "field": "Status",
        "type": "List"
      }
    },
    "bookmarks": {
      "enabled": true,
      "bookmarks": [
        {
          "name": "Overview",
          "description": "Main dashboard view"
        },
        {
          "name": "Detailed Analysis",
          "description": "Expanded analytics view"
        },
        {
          "name": "Mobile View",
          "description": "Optimized for mobile devices"
        }
      ]
    }
  },
  "responsive_design": {
    "breakpoints": {
      "mobile": {
        "max_width": "768px",
        "card_layout": "vertical",
        "table_columns": "reduced",
        "font_sizes": {
          "header": "16px",
          "body": "12px",
          "numbers": "18px"
        }
      },
      "tablet": {
        "max_width": "1024px",
        "card_layout": "2x3",
        "table_columns": "standard",
        "font_sizes": {
          "header": "18px", 
          "body": "14px",
          "numbers": "20px"
        }
      },
      "desktop": {
        "min_width": "1025px",
        "card_layout": "horizontal",
        "table_columns": "full",
        "font_sizes": {
          "header": "20px",
          "body": "14px", 
          "numbers": "24px"
        }
      }
    }
  },
  "performance_optimization": {
    "data_model": {
      "aggregations": {
        "enabled": true,
        "tables": ["GroupOutput", "Screen_Reject", "Tech_Reject"]
      },
      "relationships": [
        {
          "from_table": "GroupOutput",
          "to_table": "GroupOutput_Detailed",
          "from_column": "Candidate GGID",
          "to_column": "Candidate GGID",
          "type": "One to Many"
        }
      ]
    },
    "visual_optimization": {
      "max_visuals_per_page": 15,
      "data_reduction": "Top N",
      "cross_filtering": "enabled",
      "drill_through": "enabled"
    }
  },
  "deployment_settings": {
    "workspace": {
      "name": "Feedback Dashboard Workspace",
      "permissions": ["Viewer", "Contributor", "Admin"],
      "sharing": {
        "app_workspace": true,
        "publish_to_web": false,
        "embed_in_sharepoint": true
      }
    },
    "refresh_schedule": {
      "frequency": "Daily",
      "time": "06:00 AM",
      "timezone": "UTC",
      "retry_count": 3
    },
    "security": {
      "row_level_security": false,
      "data_classification": "Internal",
      "audit_logging": true
    }
  }
}
