    "data_table": {
      "visual_type": "Table",
      "columns": [
        {
          "field": "Candidate GGID",
          "width": "120px",
          "text_align": "left"
        },
        {
          "field": "Candidate Name", 
          "width": "200px",
          "text_align": "left"
        },
        {
          "field": "Row Count",
          "display_name": "No of Opportunities",
          "width": "150px",
          "text_align": "center",
          "conditional_formatting": {
            "type": "data_bars",
            "color_scale": {
              "min_color": "#fee2e2",
              "max_color": "#dc2626"
            }
          }
        }
      ],
      "formatting": {
        "header_background": "#f8fafc",
        "header_text_color": "#374151",
